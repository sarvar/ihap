    "jsdom": "^9.0.0",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "webpack": "^1.12.9",
    "webpack-closure-compiler": "^2.0.2",
    "webpack-dev-server": "^1.14.1"
